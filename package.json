{
  "name": "react-realm-ctx",
  "version": "0.1.0",
  "description": "more react-like experience for realm",
  "keywords": [
    "react",
    "react-native",
    "realm"
  ],
  "author": "Jan Czizikow <jan.czizikow@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/janczizikow/react-realm-ctx",
  "repository": "github:janczizikow/react-realm-ctx",
  "bugs": "https://github.com/janczizikow/react-realm-ctx/issues",
  "main": "dist",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "tsc -d",
    "prepublishOnly": "yarn build",
    "format": "prettier --config .prettierrc --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint . --cache",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage --detectOpenHandles"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "realm": ">=3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.10",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-test-renderer": "16.11.0",
    "realm": "3.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.2"
  }
}
